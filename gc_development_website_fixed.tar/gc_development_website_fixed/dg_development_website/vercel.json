
{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd app && yarn install && yarn build",
  "outputDirectory": "app/.next",
  "installCommand": "cd app && yarn install",
  "devCommand": "cd app && yarn dev",
  "functions": {
    "app/**": {
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://gc-development-group.vercel.app"
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/app/$1"
    }
  ]
}
